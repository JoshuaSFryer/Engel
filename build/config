[INFO]
Token = YOUR TOKEN GOES HERE
Master = USER WITH ALL COMMAND PRIVILEGES GOES HERE


[LOG]
# CRITICAL=50, ERROR=40, WARNING=30, INFO=20, DEBUG=10, NOTSET = 0
LogLevel = 20

[RSSH]
RemotePort = PORT USED FOR MAPPING YOUR CONNECTION INTO THE REMOTE SERVER
SshPort = PORT USED FOR YOUR MACHINE SSH CONNECTIONS
RemoteUser = USER IN THE REMOTE SERVER
RemoteServer = ADDRESS OF THE REMOTE SERVER WHERE YOU WILL FORWARD YOUR CONNECTION
